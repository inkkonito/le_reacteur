{
  "kind": "collectionType",
  "collectionName": "tickets",
  "info": {
    "singularName": "ticket",
    "pluralName": "tickets",
    "displayName": "Ticket"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "email": {
      "type": "email",
      "required": true
    },
    "username": {
      "type": "string",
      "required": true
    },
    "category": {
      "type": "enumeration",
      "enum": [
        "orchestre",
        "mezzanine"
      ],
      "required": true
    },
    "seats": {
      "required": true,
      "min": 1,
      "max": 4,
      "type": "integer"
    },
    "event": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::event.event"
    }
  }
}
